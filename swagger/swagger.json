                "operationId": "BindController.Delete",
                "parameters": [
                    {
                        "in": "header",
                        "name": "Authorization",
                        "description": "token for login api",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "name",
                        "description": "the form name",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "query",
                        "name": "rule",
                        "description": "the rule name",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} 删除成功"
                    },
                    "400": {
                        "description": "请求参数为空"
                    },
                    "401": {
                        "description": "token有误"
                    },
                    "404": {
                        "description": "规则不存在"
                    }
