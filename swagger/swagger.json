        "/analy/": {
            "post": {
                "tags": [
                    "analy"
                ],
                "description": "根据业务数据查找匹配规则",
                "operationId": "AnalyController.Analy",
                "parameters": [
                    {
                        "in": "header",
                        "name": "Authorization",
                        "description": "token for login api",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "body",
                        "name": "body",
                        "description": "the rule name and business data",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/data"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "{string} 数值"
                    },
                    "201": {
                        "description": "{string} json"
                    },
                    "401": {
                        "description": "token有误"
                    },
                    "404": {
                        "description": "规则不匹配"
                    }
                }
            }
        },
        "/bind/": {
